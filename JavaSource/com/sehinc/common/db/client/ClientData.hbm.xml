<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.sehinc.common.db.client.ClientData"
           table="CLIENT">
        <id name="id"
            type="integer"
            column="id">
            <generator class="identity" />
        </id>
        <property name="shortName"
                  type="string"
                  column="SHORT_NAME" />
        <property name="name"
                  type="string"
                  column="NAME" />
        <property name="addressId"
                  type="integer"
                  column="ADDRESS_ID" />
        <property name="phone"
                  type="string"
                  column="PHONE" />
        <property name="contactId"
                  type="integer"
                  column="CONTACT_ID" />
        <property name="contactName"
                  type="string"
                  column="CONTACT_NAME" />
        <property name="contactPhone"
                  type="string"
                  column="CONTACT_PHONE" />
        <property name="federalTaxId"
                  type="string"
                  column="FEDERAL_TAX_ID" />
        <property name="stateTaxId"
                  type="string"
                  column="STATE_TAX_ID" />
        <property name="majorWatershed"
                  type="string"
                  column="MAJOR_WATERSHED" />
        <property name="logoLocation"
                  type="string"
                  column="LOGO_LOC" />
        <property name="mapLogoLocation"
                  type="string"
                  column="MAP_LOGO_LOC" />
        <property name="webPage"
                  type="string"
                  column="WEB_PAGE" />
        <property name="comment"
                  type="string"
                  column="COMMENT" />
        <many-to-one name="status"
                     class="com.sehinc.erosioncontrol.db.code.StatusCodeData"
                     column="STATUS_CD"
                     lazy="false" />
        <property name="defaultPlanId"
                  type="integer"
                  column="DEFAULT_PLAN_ID" />
        <property name="defaultProgramId"
                  type="integer"
                  column="DEFAULT_PROGRAM_ID" />
        <property name="createTimestamp"
                  type="timestamp"
                  column="create_ts" />
        <property name="updateTimestamp"
                  type="timestamp"
                  column="update_ts" />
        <property name="updateUserId"
                  type="integer"
                  column="update_user_id" />
        <join table="CLIENT_ADMIN_SETTINGS"
              optional="false">
            <key column="CLIENT_ID" />
            <component name="clientAdminSettings"
                       class="com.sehinc.common.db.client.ClientAdminSettingsData">
                <property name="isProjectStatusEmailsEnabled"
                          access="field"
                          column="PROJECT_STATUS_EMAIL_ENABLED"
                          type="boolean" />
                <property name="isInspectionOverdueNotificationEnabled"
                          access="field"
                          column="INSP_OVERDUE_NOTIF_ENABLED"
                          type="boolean" />
                <property name="isSecondInspectionOverdueNotificationEnabled"
                          access="field"
                          column="SECOND_INSP_OVERDUE_NOTIF_ENABLED"
                          type="boolean" />
                <property name="inspectionOverdueInitialThreshold"
                          column="INSP_OVERDUE_INIT_THRESHOLD"
                          type="int" />
                <property name="inspectionOverdueInitialMessage"
                          column="INSP_OVERDUE_INIT_MSG"
                          type="string" />
                <property name="inspectionOverdueSecondThreshold"
                          column="INSP_OVERDUE_SEC_THRESHOLD"
                          type="int" />
                <property name="inspectionOverdueSecondMessage"
                          column="INSP_OVERDUE_SEC_MSG"
                          type="string" />
                <property name="inspectionCertificationEnabled"
                          column="INSP_CERTIF_ENABLED"
                          type="boolean" />
                <property name="inspectionCertificationMessage"
                          column="INSP_CERTIF_MSG"
                          type="string" />
            </component>
        </join>
    </class>
</hibernate-mapping>
