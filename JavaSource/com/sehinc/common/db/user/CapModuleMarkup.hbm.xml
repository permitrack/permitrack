<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sehinc.common.db.user.CapModuleMarkup"
           table="CAP_MODULE_MARKUP">
        <id name="id"
            type="integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="moduleId"
                  type="integer"
                  column="MODULE_ID" />
        <property name="markupId"
                  type="integer"
                  column="MARKUP_ID" />
    </class>
</hibernate-mapping>
