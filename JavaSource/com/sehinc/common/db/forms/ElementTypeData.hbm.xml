<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.sehinc.common.db.forms.ElementTypeData"
           table="ELEMENT_TYPE">
        <id name="id"
            type="integer"
            column="ELEMENT_TYPE_ID">
            <generator class="identity" />
        </id>
        <property name="htmlType"
                  type="string"
                  column="HTML_TYPE" />
        <property name="name"
                  type="string"
                  column="NAME" />
    </class>
</hibernate-mapping>

