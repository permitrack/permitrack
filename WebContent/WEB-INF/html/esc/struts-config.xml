<?xml version="1.0" encoding="ISO-8859-1" ?><!DOCTYPE struts-config PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
    "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
    <global-forwards>
        <forward name="erosioncontrol"
                 path="/defaultaction.do"
                 redirect="false" />
    </global-forwards>
    <action-mappings>
        <action path="/defaultaction"
                type="com.sehinc.erosioncontrol.action.base.DefaultAction" />
    </action-mappings>
    <controller processorClass="org.apache.struts.action.RequestProcessor"
                contentType="text/html" />
    <message-resources parameter="com.sehinc.erosioncontrol.resources.ApplicationResources"
                       null="false" />
</struts-config>
