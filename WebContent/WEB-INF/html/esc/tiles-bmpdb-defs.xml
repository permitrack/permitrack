<!DOCTYPE tiles-definitions PUBLIC
    "-//Apache Software Foundation//DTD Tiles Configuration 1.3//EN"
    "http://struts.apache.org/dtds/tiles-config_1_3.dtd">
<tiles-definitions>
    <definition name="BMPDB.baseTitledLayout"
                extends="html.baseTitledLayout">
        <put name="primaryMenu"
             value="/html/esc/primarynavigation.jsp" />
        <put name="secondaryMenu"
             value="/html/esc/secondarynavigation.jsp" />
    </definition>
    <definition name="bmp.library.list.page"
                extends="BMPDB.baseTitledLayout">
        <put name="title"
             value="BMP Library List" />
        <put name="description"
             value="Listing of all library BMPs grouped by category." />
        <put name="content"
             value="/html/esc/bmpdb/bmplibrarylist.jsp" />
    </definition>
    <definition name="bmp.library.edit.page"
                extends="BMPDB.baseTitledLayout">
        <put name="title"
             value="BMP Library Edit" />
        <put name="description"
             value="Update a BMP library." />
        <put name="content"
             value="/html/esc/bmpdb/bmplibraryedit.jsp" />
    </definition>
    <definition name="bmp.load.library.page"
                extends="BMPDB.baseTitledLayout">
        <put name="title"
             value="BMP Library List" />
        <put name="description"
             value="Best Management Practice Database Listing" />
        <put name="content"
             value="/html/esc/bmpdb/bmploadlibrarylist.jsp" />
    </definition>
    <definition name="bmp.add.library.page"
                extends="BMPDB.baseTitledLayout">
        <put name="title"
             value="BMP Library List" />
        <put name="description"
             value="Save BMP List as Library" />
        <put name="content"
             value="/html/esc/bmpdb/bmpaddlibrarylist.jsp" />
    </definition>
</tiles-definitions>
